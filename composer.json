{
  "name": "artisanfw/token-manager",
  "description": "Create and redeem storable tokens in your application.",
  "type": "library",
  "authors": [
    {
      "name": "Airam Batista",
      "email": "airamb@gmail.com"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/artisanfw/doctrine.git"
    }
  ],
  "require": {
    "php": ">=8.3",
    "artisanfw/doctrine": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "Artisan\\TokenManager\\": "src/",
      "Tests\\": "tests/"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require-dev": {
    "phpunit/phpunit": "^12.2"
  },
  "scripts": {
    "test": "phpunit"
  }
}
